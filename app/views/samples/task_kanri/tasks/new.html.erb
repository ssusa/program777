<h2>登録画面</h2>
<div class="waku">
  <%= form_for @task do |f| %>
    <%= render "form", f: f %>
    <div class="row well" style="margin:20px 0 0 0;padding:10px;">
      <div class="col-sm-12">
        <%= link_to "キャンセル", samples_task_kanri_tasks_path, class:"btn btn-default", style:"float:left" %>
        <%= f.submit "登録する", class:"btn btn-primary", style:"float:right" %>
      </div>
    </div>
  <% end %>
</div>